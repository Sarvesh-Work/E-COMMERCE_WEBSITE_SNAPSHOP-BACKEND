{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes":[
        {
            "src": "/products",
            "dest": "index.js"
        },
        {
            "src": "/products/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/brands",
            "dest": "index.js"
        },
        {
            "src": "/brands/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/categories",
            "dest": "index.js"
        },
        {
            "src": "/categories/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/user",
            "dest": "index.js"
        },
        {
            "src": "/user/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/auth",
            "dest": "index.js"
        },
        {
            "src": "/auth/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/cart",
            "dest": "index.js"
        },
        {
            "src": "/cart/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/order",
            "dest": "index.js"
        },
        {
            "src": "/order/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/",
            "dest": "dist/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "dist/$1"
        },{
            "src": "/webhook",
            "dest": "index.js"
        }, {
            "src": "/create-payment-intent",
            "dest": "index.js"
        }
    ]
  

  }
